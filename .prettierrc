{
	"semi": false,
	"tabWidth": 2,
	"singleQuote": true,
	"trailingComma": "all",
	"printWidth": 80,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"arrowParens": "always",
	"endOfLine": "auto",
	"importOrder": [
		"^(react|next?/?([a-zA-Z/]*))$",
		"<THIRD_PARTY_MODULES>",
		"^@/(.+)$",
		"^[./]((?!scss).)*$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": [
		"@trivago/prettier-plugin-sort-imports",
		"prettier-plugin-tailwindcss"
	],
	"jsxSingleQuote": true,
	"embeddedLanguageFormatting": "auto",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"useTabs": true,
	"vueIndentScriptAndStyle": false
}
